{
  "name": "ui_suite_uswds_starterkit",
  "version": "1.0.0",
  "description": "Custom theme for using USWDS library",
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "gulp",
    "build:uswds:init": "gulp uswdsInit",
    "build:uswds:update": "gulp updateUswds",
    "build:css": "gulp compileCss",
    "build:js": "gulp compileJs",
    "build:all": "gulp compileAll",
    "watch": "gulp watch",
    "lint:css": "stylelint \"**/*.scss\"",
    "lint:css:fix": "stylelint \"**/*.scss\" --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint --fix .",
    "postinstall": "patch-package"
  },
  "paths": {
    "dist": {
      "assets": "./assets",
      "uswds": "./assets/uswds",
      "css": "./assets/css",
      "js": "./assets/js",
      "img": "./images"
    },
    "scss": [
      "./sass/**/*.scss"
    ],
    "js": [
      "./js/**/*.js"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@uswds/compile": "^1.2.1",
    "autoprefixer": "^10.4.13",
    "del": "^7.1.0",
    "eslint": "^9.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-yml": "^1.14.0",
    "gulp": "^5.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^6.0.0",
    "gulp-uglify-es": "^3.0.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.0.0",
    "sass": "^1.50.0",
    "sass-embedded": "^1.58.0",
    "stylelint": "^16.3.0",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^6.2.0"
  },
  "dependencies": {
    "@uswds/uswds": "^3.12.0"
  }
}
